   <script type="text/javascript">

       // Variável que controla se a publicidade já foi chamada ou não
       var pubLeaderboardImpressa = false;

        var leaderboardHome = function() {

            // Elmento que irá conter a publicidade (div do dfp)
            var elemento = document.getElementById("div-gpt-ad-1487949073580-0");

            // verifica se o elemento está visível em ecrã e se não tiver sido impressa ainda publicidade chama o dfp
            if (pubLeaderboardImpressa === false && elementoVisivelEmEcra(elemento)) {

                // carregamento do script do dfp
                googletag.cmd.push(function () {
                    googletag.pubads().refresh([leaderboardLl]);
                });

                // Depois de chamar o dfp colocamos a variável a true
                pubLeaderboardImpressa = true;

                // Removemos o evento de scroll da página, para não ficar a acumular eventos que já não são precisos
                window.removeEventListener('scroll', leaderboardHome);
            }
        }
       // Adicionar a função de evento de scroll
        window.addEventListener('scroll', leaderboardHome);
    </script>


