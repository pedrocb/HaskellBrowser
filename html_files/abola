<script>

    $(document).ready(function () {
        $('.clube').mouseover(function () {
            $(this).animate({
                width: '30px',
                height: '30px',
                marginLeft: '-1px',
                marginTop: '-3px'
            }, 100);
        });

        $('.clube').mouseleave(function () {
            $(this).animate({
                width: '25px',
                height: '25px',
                marginLeft: '0px',
                marginTop: '0px'
            }, 100);
        });
    });

</script>

