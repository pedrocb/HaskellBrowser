	$(document).ready(function () {

		// NOTICIAS
		$(".nav0, .nav7").hover(function () {
			$(this).animate({
				'backgroundColor': '#000000'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#000000'
			}, 100);
		});


		// NACIONAL
		$(".nav1").hover(function () {
			$(this).animate({
				'backgroundColor': '#00CCFF'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#00CCFF'
			}, 100);
		});

		// INTERNACIONAL
		$(".nav2").hover(function () {
			$(this).animate({
				'backgroundColor': '#6666CC'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#6666CC'
			}, 100);
		});

		// MODALIDADES
		$(".nav3").hover(function () {
			$(this).animate({
				'backgroundColor': '#FF6633'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#FF6633'
			}, 100);
		});

		// MOTORES
		$(".nav4").hover(function () {
			$(this).animate({
				'backgroundColor': '#FDCA68'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#FDCA68'
			}, 100);
		});

		// SELECAO
		$(".nav5").hover(function () {
			$(this).animate({
				'backgroundColor': '#990000'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

		}, function () {
			$(this).animate({
				'backgroundColor': '#FFFFFF'
			}, 100);
			$(this).find("a").animate({
				'color': '#990000'
			}, 100);
		});

		// EDICAO DIGITAL
		$(".nav8").hover(function () {
			$(this).animate({
				'backgroundColor': '#FC0000'
			}, 100);

			$(this).find("a").animate({
				'color': '#FFFFFF'
			}, 100);

