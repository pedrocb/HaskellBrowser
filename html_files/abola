<script>
    $(document).ready(function () {

        $("#menu").click(function () {
            $('#menu-principal-hp').slideToggle("fast", function () {
                if ($(this).is(":visible")) {
                    $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu_close.png');
                } else {
                    $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu.png');
                }
            });
        });

        $("#menu").hover(
              function () {
                  if ($("#menu-principal-hp").is(":visible")) {
                      $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu_close_hover.png');
                  } else {
                      $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu_hover.png');
                  }

              }, function () {
                  if ($("#menu-principal-hp").is(":visible")) {
                      $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu_close.png');
                  } else {
                      $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu.png');
                  }
              }
            );
    });

    $(document).keyup(function (e) {
        if (e.keyCode == 27) {
            $('#menu-principal-hp').slideUp("fast", function () {
                $(".abola-menu").attr('src', 'http://www.abola.pt/img/menu.png');
            });

        }
    });

    //$(document).mouseup(function (e) {
    //    var container = $('#menu-principal-hp');

    //    if (!container.is(e.target) 
    //        && container.has(e.target).length === 0) 
    //    {
    //        container.slideUp("fast");
    //    }
    //});

</script>

