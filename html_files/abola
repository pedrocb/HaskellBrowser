<script type="text/javascript">
    $(document).ready(function () {
        $('.bxslider_Destaque').visibility = "visible";
        var slider = $('.bxslider_Destaque').bxSlider({
            auto: true,
            pager: false,
            speed: 200,
            captions: true,
            nextSelector: '#slider-next',
            prevSelector: '#slider-prev',
            nextText: '<img src="img/seta_direita_cinza_medio.png" />',
            prevText: '<img src="img/seta_esquerda_cinza_medio.png" />',
            onSliderLoad: function () {
                $("#foto-destaque").css("visibility", "visible");
                $('.botoes-destaque li:first-child div').addClass('circulo-selected');
                $('.botoes-destaque li:not(:first) div').addClass('cinza-bg');
            },
            onSlideBefore: function (currentSlideNumber, totalSlideQty, currentSlideHtmlObject) {
                $('.botoes-destaque li div').removeClass('circulo-selected');
                $('.botoes-destaque li div').eq(currentSlideHtmlObject).addClass('circulo-selected');
                $('.botoes-destaque li div').not(currentSlideHtmlObject).addClass('cinza-bg');
                $('.botoes-destaque li div').eq(currentSlideHtmlObject).removeClass('cinza-bg');
            }
        });

        // PARA BOTOES COM CORES
        $('.botoes-destaque li').click(function () {
            $(this).children().addClass("circulo-selected");
            var slide = $(this).index();
            slider.goToSlide(slide);
            slider.stopAuto();
            restart = setTimeout(function () {
                slider.startAuto();
            }, 200);
            return false;
        });

        $('.botoes-destaque li').hover(
          function () {
              $(this).children().removeClass("cinza-bg");
          }, function () {
              if (!$(this).children().hasClass('circulo-selected')) {
                  $(this).children().addClass("cinza-bg");
              }
          }
        );

        $("#botoes-destaque .bx-prev").hover(function () {
            $(this).find("img").attr("src", "img/seta_esquerda_cinza.png");
        }, function () {
            $(this).find("img").attr("src", "img/seta_esquerda_cinza_medio.png");
        });

        $("#botoes-destaque .bx-next").hover(function () {
            $(this).find("img").attr("src", "img/seta_direita_cinza.png");
        }, function () {
            $(this).find("img").attr("src", "img/seta_direita_cinza_medio.png");
        });

        // PARA TOOLTIPS
        //$("#foto-destaque").on('mouseover', '.bx-pager-item', function () {


        //    var i = parseInt($(this).children().attr("data-slide-index")) + 1;

        //    var imagem = $('#destaque' + i + " li img").attr("src");
        //    var texto = $('#destaque' + i + " #titulo-destaque").text();
        //    $(this).tooltipster({
        //        content: $('<span><img src="' + imagem + '" width=100 /> <strong>' + texto + '</strong></span>'),
        //        position: 'bottom',
        //        delay: 0,
        //        maxWidth: 120,
        //        speed: 0
        //    });
        //});

        // PARA MOUSEOVER
        //$("#foto-destaque").on('mouseover', '.bx-pager-item', function () {

        //    var i = $(this).index();
        //    slider.goToSlide(i);
        //    slider.stopAuto();
        //    restart = setTimeout(function () {
        //        slider.startAuto();
        //    }, 200);
        //    return false;
        //});


        // PARA NAVEGAÇÃO COM SETAS DIRECIONAIS
        //$(document).keydown(function (e) {
        //    var current = slider.getCurrentSlide();
        //    var slideQty = slider.getSlideCount();
        //    switch (e.which) {
        //        case 37: // left
        //            console.log(current);
        //            if ((current - 1) >= 0) {
        //                slider.goToSlide(current - 1);
        //                slider.stopAuto();
        //            }
        //            else {
        //                slider.goToSlide(slideQty - 1);
        //                slider.stopAuto();
        //            }
        //            break;

        //        case 39: // right
        //            console.log(current);
        //            if ((current + 1) < slideQty) {
        //                slider.goToSlide(current + 1);
        //                slider.stopAuto();
        //            } else {
        //                slider.goToSlide(0);
        //                slider.stopAuto();
        //            }


        //            break;
        //    }
        //});
    });



