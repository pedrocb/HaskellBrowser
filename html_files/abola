<script>
    $(document).ready(function () {
        //$('.resultados-eq-casa, .resultados-eq-fora').dotdotdot();

        $(".jornada-dois").hide();
        $(".jornada-quatro").hide();
       

        var pathArray = window.location.pathname.split('/');

        if (pathArray[3] == "mocambique") {

            $(".liga-1").hide();
            $(".liga-2").show();

            $(".jornadas").hide();
            $(".jornadas-2").show();

            $(".classif .tab-3").removeClass("inactive");
            $(".classif .tab-3").addClass("active");
            $(".classif .tab-4").removeClass("active");
            $(".classif .tab-4").addClass("inactive");
            

        }
       

        $(".classif .tab-1").click(function () {
            $(".jornadas").show();
            $(".classif-jorn").hide();
            $(".jornadas-2").hide();
            $(".classif-jorn-2").hide();
            $(".jornada-dois").hide();
            $(".jornada-um").show();

            $(".classif .tab-1").removeClass("inactive");
            $(".classif .tab-1").addClass("active");
            $(".classif .tab-2").removeClass("active");
            $(".classif .tab-2").addClass("inactive");
            $(".classif .tab-5").removeClass("active");
            $(".classif .tab-5").addClass("inactive");
            
        });


        $(".classif .tab-2").click(function () {
            $(".jornadas").hide();
            $(".classif-jorn").show();
            $(".jornadas-2").hide();
            $(".classif-jorn-2").hide();

            $(".classif .tab-2").removeClass("inactive");
            $(".classif .tab-2").addClass("active");
            $(".classif .tab-1").removeClass("active");
            $(".classif .tab-1").addClass("inactive");
            $(".classif .tab-5").removeClass("active");
            $(".classif .tab-5").addClass("inactive");
        });


        $(".classif .tab-3").click(function () {
            $(".jornadas-2").show();
            $(".classif-jorn-2").hide();
            $(".jornadas").hide();
            $(".classif-jorn").hide();
            $(".jornada-tres").show();
            $(".jornada-quatro").hide();

            $(".classif .tab-3").removeClass("inactive");
            $(".classif .tab-3").addClass("active");
            $(".classif .tab-4").removeClass("active");
            $(".classif .tab-4").addClass("inactive");
            $(".classif .tab-6").removeClass("active");
            $(".classif .tab-6").addClass("inactive");
        });


        $(".classif .tab-4").click(function () {
            $(".jornadas-2").hide();
            $(".classif-jorn-2").show();
            $(".jornadas").hide();
            $(".classif-jorn").hide();

            $(".classif .tab-4").removeClass("inactive");
            $(".classif .tab-4").addClass("active");
            $(".classif .tab-3").removeClass("active");
            $(".classif .tab-3").addClass("inactive");
            $(".classif .tab-6").removeClass("active");
            $(".classif .tab-6").addClass("inactive");
        });

        $(".classif .tab-5").click(function () {
            $(".jornadas").show();
            $(".classif-jorn").hide();
            $(".jornadas-2").hide();
            $(".classif-jorn-2").hide();
            $(".jornada-dois").show();
            $(".jornada-um").hide();

            $(".classif .tab-1").removeClass("active");
            $(".classif .tab-1").addClass("inactive");
            $(".classif .tab-2").removeClass("active");
            $(".classif .tab-2").addClass("inactive");
            $(".classif .tab-5").removeClass("inactive");
            $(".classif .tab-5").addClass("active");
        });

        $(".classif .tab-6").click(function () {
            $(".jornadas-2").show();
            $(".classif-jorn-2").hide();
            $(".jornadas").hide();
            $(".classif-jorn").hide();
            $(".jornada-quatro").show();
            $(".jornada-tres").hide();

            $(".classif .tab-3").removeClass("active");
            $(".classif .tab-3").addClass("inactive");
            $(".classif .tab-4").removeClass("active");
            $(".classif .tab-4").addClass("inactive");
            $(".classif .tab-6").removeClass("inactive");
            $(".classif .tab-6").addClass("active");
        });


        $("#botao-dir").click(function () {
            if (!$(".liga-2").is(":visible")) {

                if ($('.classif .tab-2').hasClass("active")) {

                    //vai para a classificacao da 2ª liga
                    $(".jornadas-2").hide();
                    //$(".classif-jorn-2").hide();
                    $(".jornadas").hide();
                    $(".classif-jorn").hide();
                    $(".classif-jorn-2").show();
                    $(".jornada-quatro").hide();
                    $(".jornada-tres").hide();

                    $(".classif .tab-4").removeClass("inactive");
                    $(".classif .tab-4").addClass("active");
                    $(".classif .tab-3").removeClass("active");
                    $(".classif .tab-3").addClass("inactive");
                    $(".classif .tab-6").removeClass("active");
                    $(".classif .tab-6").addClass("inactive");
                }
                else if ($('.classif .tab-5').hasClass("active")) {

                    //vai para a jornada seguinte da 2ª liga
                    $(".jornadas-2").show();
                    $(".classif-jorn-2").hide();
                    $(".jornadas").hide();
                    $(".classif-jorn").hide();
                    $(".classif-jorn-2").hide();
                    $(".jornada-quatro").show();
                    $(".jornada-tres").hide();

                    $(".classif .tab-4").removeClass("active");
                    $(".classif .tab-4").addClass("inactive");
                    $(".classif .tab-3").removeClass("active");
                    $(".classif .tab-3").addClass("inactive");
                    $(".classif .tab-6").removeClass("inactive");
                    $(".classif .tab-6").addClass("active");

                }


                else if ($('.classif .tab-1').hasClass("active")) {
                    //vai para as jornadas da 2ª liga
                    $(".classif-jorn-2").hide();
                    $(".jornadas").hide();
                    $(".classif-jorn").hide();
                    //$(".classif-jorn-2").hide();
                    $(".jornadas-2").show();
                    $(".jornada-quatro").hide();
                    $(".jornada-tres").show();
                   

                    $(".classif .tab-3").removeClass("inactive");
                    $(".classif .tab-3").addClass("active");
                    $(".classif .tab-4").removeClass("active");
                    $(".classif .tab-4").addClass("inactive");
                    $(".classif .tab-6").removeClass("active");
                    $(".classif .tab-6").addClass("inactive");
                }

                $(".liga-2").show();
                $(".liga-1").hide();
            }
            //$('.resultados-eq-casa, .resultados-eq-fora').dotdotdot();
        });

        $("#botao-esq").click(function () {

            if (!$(".liga-1").is(":visible")) {

                if ($('.classif .tab-4').hasClass("active")) {

                    //vai para a classificacao da 1ª liga
                    $(".jornadas-2").hide();
                    $(".classif-jorn-2").hide();
                    $(".jornadas").hide();
                    $(".classif-jorn").show();
                    $(".classif-jorn-2").hide();
                    $(".jornada-dois").hide();
                    $(".jornada-um").hide();

                    $(".classif .tab-2").removeClass("inactive");
                    $(".classif .tab-2").addClass("active");
                    $(".classif .tab-1").removeClass("active");
                    $(".classif .tab-1").addClass("inactive");
                    $(".classif .tab-5").removeClass("active");
                    $(".classif .tab-5").addClass("inactive");
                   
                }

                else if ($('.classif .tab-6').hasClass("active")) {

                    //vai para a classificacao da 1ª liga
                    $(".jornadas-2").hide();
                    $(".classif-jorn-2").hide();
                    $(".jornadas").show();
                    $(".classif-jorn").hide();
                    $(".classif-jorn-2").hide();
                    $(".jornada-dois").show();
                    $(".jornada-um").hide();

                    $(".classif .tab-2").removeClass("active");
                    $(".classif .tab-2").addClass("inactive");
                    $(".classif .tab-1").removeClass("active");
                    $(".classif .tab-1").addClass("inactive");
                    $(".classif .tab-5").removeClass("inactive");
                    $(".classif .tab-5").addClass("active");
                }

                else if ($('.classif .tab-3').hasClass("active")) {
                    //vai para as jornadas da 1ª liga
                    $(".classif-jorn-2").hide();
                    $(".jornadas").show();
                    $(".classif-jorn").hide();
                    $(".classif-jorn-2").hide();
                    $(".jornadas-2").hide();
                    $(".jornada-dois").hide();
                    $(".jornada-um").show();

                    $(".classif .tab-1").removeClass("inactive");
                    $(".classif .tab-1").addClass("active");
                    $(".classif .tab-2").removeClass("active");
                    $(".classif .tab-2").addClass("inactive");
                    $(".classif .tab-5").removeClass("active");
                    $(".classif .tab-5").addClass("inactive");
                }

                $(".liga-1").show();
                $(".liga-2").hide();
            }
           // $('.resultados-eq-casa, .resultados-eq-fora').dotdotdot();
        });
    });
   
</script>

