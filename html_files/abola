    <script>
        $(window).load(function () {
            var $sky = $(".sky");

            if ($('.sky>div').length) {
                if ($('.sky').css('display') !== 'none') {
                    $(document.body).hide();

                    var w = $(document.body).width() + ($sky.length * 122);
                    $(document.body).width(w + "px");
                    $("#wrapper").addClass("left");

                    $(document.body).show();
                }

                $(document).scroll(function () {
                    $sky.css("top", ($(document).scrollTop() + "px"));
                });
            }

            $("#hlCookie").click(function () {
                $("#div_cookie").hide();
            });

        });
    </script>


