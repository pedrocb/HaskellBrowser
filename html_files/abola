    <script>
        // ATENÇÃO! SCRIPT NECESSÁRIO PARA COLOCAR A PUBLICIDADE NA ZONA DOS DESTAQUES
        $(window).load(function () {
            // Verificar se há publicidade
            if ($('.pubAuto>div').length && $('.pubAuto').css('display') !== 'none') {
                // Limpamos o destaque
                $('.principal-destaque').remove();
            } else {
                // Se não houve publicidade escondemos a div para não ficar o buraco
                $('.pubAuto').hide();
            }
        });
